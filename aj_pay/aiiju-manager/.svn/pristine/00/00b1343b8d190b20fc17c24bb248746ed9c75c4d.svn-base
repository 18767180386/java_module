package com.aiiju.pojo;

import java.io.Serializable;
import java.util.Date;
/**
 * 
* @ClassName: Printer 
* @Description: 打印机
* @author 宗介 
* @date 2017年4月25日 下午15:16:24
 */
public class Printer implements Serializable{

    private static final long serialVersionUID = -6746798375007862423L;
    /**
     * 打印设备表主键
     */
    private Long id;
    /**
     * 打印设备名称
     */
    private String printerName;
    /**
     * 打印设备型号或者序列号
     */
    private String printerModelNum;
    /**
     * 打印设备密钥（目前365小票机是有的，没有为空）
     */
    private String printerPassword;
    /**
     * 打印设备的所属商铺
     */
    private String printerStoreId;
    
    /**
     * 与打印设备的链接状态,0，没有链接,1已链接
     */
    private String printerLinkStatus;
    
    /**
     * 打印设备的添加人
     */
    private String printerCreator;
    /**
     * 备注
     */
    private String remark;
    /**
     * 创建日期
     */
    private Date createDate;
    /**
     * 修改时间
     */
    private Date modifyDate;
    /**
     * 状态，控制该条数据是否可用或有效；0，不可用，1可用
     */
    private String status;
    
    
    
    
    
	public String getPrinterLinkStatus() {
		return printerLinkStatus;
	}
	public void setPrinterLinkStatus(String printerLinkStatus) {
		this.printerLinkStatus = printerLinkStatus;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getPrinterName() {
		return printerName;
	}
	public void setPrinterName(String printerName) {
		this.printerName = printerName;
	}
	public String getPrinterModelNum() {
		return printerModelNum;
	}
	public void setPrinterModelNum(String printerModelNum) {
		this.printerModelNum = printerModelNum;
	}
	public String getPrinterPassword() {
		return printerPassword;
	}
	public void setPrinterPassword(String printerPassword) {
		this.printerPassword = printerPassword;
	}
	
	public String getPrinterStoreId() {
		return printerStoreId;
	}
	public void setPrinterStoreId(String printerStoreId) {
		this.printerStoreId = printerStoreId;
	}
	public String getPrinterCreator() {
		return printerCreator;
	}
	public void setPrinterCreator(String printerCreator) {
		this.printerCreator = printerCreator;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public Date getModifyDate() {
		return modifyDate;
	}
	public void setModifyDate(Date modifyDate) {
		this.modifyDate = modifyDate;
	}
    
    
    
  
    
}
